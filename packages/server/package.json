{
  "name": "@bookshelf/server",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "apollo-server-express": "2.15.1",
    "express": "4.17.1",
    "graphql": "15.3.0",
    "pg": "8.2.1",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.25"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "apollo-server-testing": "2.15.1",
    "nodemon": "2.0.4",
    "sqlite3": "5.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc --skipLibCheck",
    "start": "node dist/src/index.js",
    "dev": "nodemon",
    "typecheck": "tsc --project tsconfig.json --skipLibCheck --noEmit",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "typeorm": "ts-node ../../node_modules/typeorm/cli.js --config src/database/config.ts",
    "db:seed": "yarn ts-node scripts/seed.ts"
  }
}
