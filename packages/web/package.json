{
  "name": "@bookshelf/web",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.0.0-rc.10",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-icons": "3.10.0",
    "react-router": "6.0.0-alpha.5",
    "react-router-dom": "6.0.0-alpha.5",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@graphql-codegen/cli": "1.16.2",
