{
  "name": "bookshelf",
  "version": "1.0.0",
  "repository": "git@github.com:lucassus/bookshelf.git",
  "author": "≈Åukasz Bandzarewicz <lukasz.bandzarewicz@gmail.com>",
  "private": true,
  "engines": {
    "node": "13.7"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "yarn lerna bootstrap",
    "postinstall": "yarn bootstrap",
    "clean": "lerna run clean",
    "build": "lerna run build",
    "typecheck": "lerna run typecheck",
    "lint": "lerna run lint",
    "test": "jest",
    "start": "yarn workspace @bookshelf/server start",
    "dev": "concurrently --raw --kill-others-on-fail 'yarn workspace @bookshelf/server dev' 'yarn workspace @bookshelf/web dev'"
  },
  "devDependencies": {
    "@types/jest": "26.0.4",
    "@types/node": "14.0.23",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "concurrently": "5.2.0",
    "eslint": "7.4.0",
    "eslint-config-airbnb-typescript": "8.0.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.18.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "ts-jest": "26.1.3",
    "ts-node": "8.10.2",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "lerna": "3.22.1"
  }
}
