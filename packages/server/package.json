{
  "name": "@bookshelf/server",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@bookshelf/secure-id": "1.0.0",
    "@graphql-tools/graphql-file-loader": "6.0.18",
    "@graphql-tools/load": "6.0.18",
    "@graphql-tools/load-files": "6.0.18",
    "@graphql-tools/merge": "6.0.18",
    "@graphql-tools/schema": "6.0.18",
    "@graphql-tools/utils": "6.0.18",
    "apollo-server-express": "2.16.1",
    "bcrypt": "5.0.0",
    "cookie-parser": "1.4.5",
    "dataloader": "2.0.0",
    "express": "4.17.1",
    "faker": "4.1.0",
    "graphql": "15.3.0",
    "graphql-voyager": "1.0.0-rc.31",
    "jsonwebtoken": "8.5.1",
    "pg": "8.3.2",
    "reflect-metadata": "0.1.13",
    "typedi": "0.8.0",
    "typeorm": "0.2.25",
    "typeorm-typedi-extensions": "0.2.3"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.7",
    "@types/faker": "4.1.12",
    "@types/jsonwebtoken": "8.5.0",
    "@types/lodash.merge": "4.6.6",
    "@types/supertest": "2.0.10",
    "apollo-server-testing": "2.16.1",
    "node-mocks-http": "1.9.0",
    "sqlite3": "5.0.0",
    "supertest": "4.0.2",
    "ts-node-dev": "1.0.0-pre.60"
  },
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean": "yarn clean:dist && yarn rimraf coverage",
    "copy-graphql": "find ./src/graphql -type f -name '*.graphql' -exec cp {} ./dist/{} \\;",
    "compile": "tsc -p tsconfig.build.json && yarn copy-graphql",
    "build": "yarn clean && yarn compile",
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/index.ts",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "db:seed": "yarn ts-node scripts/seed.ts"
  }
}
