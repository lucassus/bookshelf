{
  "name": "bookshelf",
  "version": "1.0.0",
  "repository": "git@github.com:lucassus/bookshelf.git",
  "author": "≈Åukasz Bandzarewicz <lukasz.bandzarewicz@gmail.com>",
  "private": true,
  "engines": {
    "node": "13.7"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "server": "yarn workspace @bookshelf/server",
    "web": "yarn workspace @bookshelf/web",
    "lint": "yarn server lint && yarn web lint",
    "test": "jest",
    "build": "yarn server build && yarn web build",
    "start": "yarn server start",
    "dev": "concurrently --raw --kill-others-on-fail 'yarn server dev' 'yarn web dev'"
  },
  "devDependencies": {
    "@types/jest": "26.0.4",
    "@types/node": "14.0.23",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "concurrently": "5.2.0",
    "eslint": "7.4.0",
    "eslint-config-airbnb-typescript": "8.0.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.18.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "ts-jest": "26.1.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.6"
  }
}
