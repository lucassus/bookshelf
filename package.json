{
  "name": "bookshelf",
  "version": "1.0.0",
  "repository": "git@github.com:lucassus/bookshelf.git",
  "author": "≈Åukasz Bandzarewicz <lukasz.bandzarewicz@gmail.com>",
  "private": true,
  "engines": {
    "node": "13.x"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "server": "yarn workspace @bookshelf/server",
    "web": "yarn workspace @bookshelf/web",
    "build": "yarn server build && yarn web build",
    "start": "node packages/server/dist/index.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server start\" \"yarn web start\""
  },
  "dependencies": {
    "concurrently": "5.1.0"
  }
}
