{
  "name": "bookshelf",
  "version": "1.0.0",
  "repository": "git@github.com:lucassus/bookshelf.git",
  "author": "≈Åukasz Bandzarewicz <lukasz.bandzarewicz@gmail.com>",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "server:test": "yarn workspace @bookshelf/server test",
    "server:start": "yarn workspace @bookshelf/server start",
    "web:test": "yarn workspace @bookshelf/web test",
    "web:start": "yarn workspace @bookshelf/web start",
    "start": "concurrently --kill-others-on-fail \"yarn server:start\" \"yarn web:start\""
  },
  "dependencies": {
    "concurrently": "5.1.0"
  }
}
