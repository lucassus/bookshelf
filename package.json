{
  "name": "bookshelf",
  "version": "1.0.0",
  "repository": "git@github.com:lucassus/bookshelf.git",
  "author": "≈Åukasz Bandzarewicz <lukasz.bandzarewicz@gmail.com>",
  "private": true,
  "engines": {
    "node": "13.7"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "server": "yarn workspace @bookshelf/server",
    "web": "yarn workspace @bookshelf/web",
    "test": "jest",
    "build": "yarn server build && yarn web build",
    "start": "yarn server start",
    "dev": "concurrently --raw --kill-others-on-fail 'yarn server dev' 'yarn web dev'"
  },
  "devDependencies": {
    "@types/jest": "25.2.2",
    "@types/node": "13.13.5",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "concurrently": "5.2.0",
    "eslint": "7.0.0",
    "eslint-config-airbnb-typescript": "7.2.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "ts-jest": "25.5.1",
    "ts-node": "8.10.1",
    "typescript": "3.9.2"
  }
}
