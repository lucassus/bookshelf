{
  "name": "@bookshelf/server",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@bookshelf/secure-id": "1.0.0",
    "@graphql-tools/graphql-file-loader": "6.0.13",
    "@graphql-tools/load": "6.0.12",
    "@graphql-tools/schema": "6.0.14",
    "apollo-server-express": "2.15.1",
    "dataloader": "2.0.0",
    "express": "4.17.1",
    "graphql": "15.3.0",
    "pg": "8.3.0",
    "reflect-metadata": "0.1.13",
    "typeorm": "0.2.25"
  },
  "devDependencies": {
    "@types/express": "4.17.7",
    "apollo-server-testing": "2.15.1",
    "nodemon": "2.0.4",
    "sqlite3": "5.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc -p tsconfig.build.json && cp src/graphql/*.graphql dist/src/graphql",
    "build": "yarn clean && yarn compile",
    "start": "node dist/src/index.js",
    "dev": "nodemon",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "typeorm": "ts-node ../../node_modules/typeorm/cli.js --config src/database/config.ts",
    "db:seed": "yarn ts-node scripts/seed.ts"
  }
}
