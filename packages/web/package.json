    "@types/react-router-dom": "5.1.5",
    "@types/webpack": "4.41.19",
    "babel-loader": "8.1.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.6.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.5",
    "eslint-plugin-testing-library": "3.3.1",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "sass": "1.26.9",
    "sass-loader": "9.0.1",
    "style-loader": "1.2.1",
    "ts-loader": "7.0.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "codegen": "graphql-codegen --config codegen.yml",
    "build": "yarn codegen && yarn webpack --config webpack.config.js --mode production",
    "dev-server": "webpack-dev-server --mode development",
    "dev": "concurrently --raw --kill-others-on-fail 'yarn codegen --watch' 'yarn dev-server'",
    "test": "jest",
    "typecheck": "tsc --project tsconfig.json --skipLibCheck --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "storybook": "start-storybook --port 6006 --ci",
    "storybook:build": "build-storybook"
  }
}
