{
  "name": "@bookshelf/fake-server",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@bookshelf/secure-id": "workspace:*",
    "@bookshelf/string-utils": "workspace:*",
    "@graphql-tools/mock": "6.2.1",
    "@graphql-tools/schema": "6.2.1",
    "@types/faker": "5.1.0",
    "apollo-server": "2.17.0",
    "faker": "5.1.0",
    "graphql": "15.3.0",
    "ts-node-dev": "1.0.0-pre.62"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "eslint": "7.8.1",
